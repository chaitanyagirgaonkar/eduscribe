{
    "version": 2,
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "^/api/(.*)", "dest": "https://eduscribe.onrender.com/api/$1" },
      { "src": "/(.*)", "dest": "/dist/$1" }
    ]
}
  
